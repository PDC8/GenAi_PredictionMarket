{
  "name": "agentic-prediction-terminal",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "node scripts/check-node-version.mjs",
    "dev": "env -u __NEXT_DEVTOOL_SEGMENT_EXPLORER next dev",
    "dev:clean": "rm -rf .next && env -u __NEXT_DEVTOOL_SEGMENT_EXPLORER next dev",
    "build": "next build",
    "start": "next start",
    "worker": "tsx scripts/worker.ts",
    "db:init": "tsx scripts/init-db.ts",
    "db:studio": "drizzle-kit studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "better-sqlite3": "^11.8.1",
    "drizzle-orm": "^0.44.5",
    "next": "15.2.4",
    "node-cron": "^4.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@types/node": "^22.13.13",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "drizzle-kit": "^0.31.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
